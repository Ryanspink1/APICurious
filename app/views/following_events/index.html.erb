<h2>Events from Users You Follow</h2>

<div class="table_div">
<table class="table table-striped", id="repo_table">
  <thead class="thead-inverse">
    <tr>
      <th>Type</th>
      <th>User</th>
      <th>Link to Repo</th>
    </tr>
  </thead>
  <tbody>
    <% @following_events.each do |follow| %>
    <tr>
      <th scope="row"><%= follow.type %> </th>
      <td><%= follow.actor[:login] %></td>
      <td> <%=  link_to follow.repo[:name], "https://github.com/#{follow.repo[:name]}"%></td>
    </tr>
    <% end %>
  </tbody>
</table>
</div>
